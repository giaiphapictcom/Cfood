@using V308CMS.Admin.Helpers.Url
@model V308CMS.Data.ProductPage
@{
    ViewBag.Title = "Nhà Sản Xuất";
    ViewBag.LinkBack = "<li><a href=" + Url.ProductManufacturerCreateUrl()+">Nhà Sản Xuất</a> </li>";
    ViewBag.MenuValue = 1;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <div>
        <div id="close" class="mheader">
            <a href="@Url.ProductManufacturerCreateUrl()" title="Thêm mới">
                <img src="@Url.CreateNewImageUrl()" title="Thêm mới" />
            </a>
        </div>
        <hr />
        <div style="width: 100%;">
            <div class="RadGrid RadGrid_MetroTouch table table-striped table-bordered table-hover dataTable" tabindex="0">
                @Html.Raw(Model.Html)
            </div>
        </div>
        <div class="wpmn_page_navi">
            <a href="@Url.ProductManufacturerIndexUrl(new {pPage =Model.BackPage})">Back</a>
                <span class="current">@Model.Page</span>
            <a href="@Url.ProductManufacturerIndexUrl(new {pPage =Model.NextPage})">Next</a>
        </div>
        <hr />
    </div>
    <div style="display: none;" role="status" aria-hidden="true">
        <div class="area"></div>
        <div class="form-loading"></div>
    </div>
</div>
