@using V308CMS.Admin.Helpers.Url
@model V308CMS.Data.AccountPage
@{
    ViewBag.Title = "Tài khoản khách hàng";
    ViewBag.LinkBack = "<li><a href=" + Url.AccountIndexUrl() +">Tài khoản khách hàng</a> </li>";
    ViewBag.MenuValue = 6;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <div>
        <div id="close" class="mheader">
            <a href="javascript:void(0);">
                <img src="@Url.CreateNewImageUrl()" alt="Thêm mới"/>
            </a>
        </div>
        <div style="margin-top:16px;">
        </div>
        <hr />
        <div style="width: 100%;">
            <div class="RadGrid RadGrid_MetroTouch table table-striped table-bordered table-hover dataTable" tabindex="0">
                @Html.Raw(Model.Html)
            </div>
        </div>
        <div class="wpmn_page_navi">
            <a href="@Url.AccountIndexUrl(new {pPage = Model.BackPage, pType = Model.TypeId})">Back</a><span class="current">@Model.Page</span>
            <a href="@Url.AccountIndexUrl(new {pPage = Model.NextPage, pType =Model.TypeId})">Next</a>
        </div>
        <hr />
    </div>
    <div style="display: none;" role="status" aria-hidden="true">
        <div class="area"></div>
        <div class="form-loading"></div>
    </div>
</div>
