	        <div class="collapse navbar-collapse">@Html.Action("MainMenu", "Block")</div>
	    </div>
    </nav>
    <div class="main-content">
	    <div class="container">
		    <div class="row">
                <div class="col-lg-3 col-md-12 col-12 mb-4">
                    <div id="leftcol"> @Html.Action("LeftColumn", "Block")</div>
                </div>
                <div class="col-lg-9 col-md-12 col-12 ">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    @Html.Action("Footer", "Block")
